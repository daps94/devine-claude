{
  "name": "devine-claude",
  "version": "1.0.0",
  "description": "Devine orchestration for Claude Code - Orchestrate multiple Claude instances as a collaborative AI development team",
  "keywords": [
    "claude",
    "ai",
    "swarm",
    "mcp",
    "orchestration",
    "anthropic",
    "openai",
    "agent"
  ],
  "homepage": "https://github.com/daps94/devine-claude",
  "bugs": {
    "url": "https://github.com/daps94/devine-claude/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daps94/devine-claude.git"
  },
  "license": "MIT",
  "author": "Your Name",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "devine-claude": "./bin/devine.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "dev": "tsx src/cli/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint && npm run typecheck && npm run test",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.3",
    "commander": "^11.1.0",
    "dotenv": "^16.4.1",
    "execa": "^8.0.1",
    "globby": "^14.0.0",
    "inquirer": "^9.2.14",
    "js-yaml": "^4.1.0",
    "jsonlines": "^0.1.1",
    "openai": "^4.28.0",
    "ora": "^8.0.1",
    "proper-lockfile": "^4.1.2",
    "signal-exit": "^4.1.0",
    "split2": "^4.2.0",
    "strip-ansi": "^7.1.0",
    "table": "^6.8.1",
    "tmp": "^0.2.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^30.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.11.16",
    "@types/proper-lockfile": "^4.1.4",
    "@types/signal-exit": "^3.0.4",
    "@types/split2": "^4.2.3",
    "@types/tmp": "^0.2.6",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "nock": "^13.5.1",
    "prettier": "^3.2.5",
    "stdout-stderr": "^0.1.13",
    "ts-jest": "^29.1.2",
    "tsup": "^8.0.2",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
